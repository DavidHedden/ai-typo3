{
	"name": "aimeos/ai-typo3",
	"description": "Aimeos extension for integration into TYPO3",
	"keywords": ["aimeos", "adapter", "typo3", "cms"],
	"homepage": "https://aimeos.org",
	"type": "aimeos-extension",
	"license": "LGPL-3.0",
	"support": {
		"source": "https://github.com/Aimeos/ai-typo3",
		"issues": "https://github.com/Aimeos/ai-typo3/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/aimeos/php-coveralls.git"
		}
	],
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.2.0"
	},
	"require-dev": {
		"satooshi/php-coveralls": "dev-master"
	},
	"autoload": {
		"psr-0": {
			"MShop": "lib/custom/src",
			"MW": "lib/custom/src"
		},
		"classmap": [
			"lib/custom/src"
		]
	}
}